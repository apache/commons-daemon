SUBDIRS = native

all: native/all

native/all:
	$(MAKE) -C native all

clean:
	$(MAKE) -C native clean

distclean: clean
	rm -f config.cache
	rm -f config.log
	rm -f config.status
	rm -f native/Makefile
	rm -f Makefile
	rm -f Makedefs

realclean: distclean
	rm -f configure
